      $('canvas').mousemove( function(e) {
      if ( flag ) {
      var endX = e.pageX - $('canvas').offset().left - offset;
      var endY = e.pageY - $('canvas').offset().top - offset;
      context.beginPath();  
      context.moveTo( startX, startY );
      context.lineTo( endX, endY );  
      context.stroke();
      startX = endX;
      startY = endY;
      }
      });

      $('canvas').on('mouseup', function() {
      flag = false;
      });

      $('canvas').on('mouseleave', function() {
      flag = false;
      });
      
      $('#color li').click( function() {
      context.strokeStyle = $(this).css('background-color');
      if( context.strokeStyle == "#ffffff" ) {
      context.lineWidth = 10;
      } else {
      context.lineWidth = 2;
      }
      });

      $('#clear').click( function(e) {
      e.preventDefault();
      context.clearRect( 0, 0, $('canvas').width(), $('canvas').height() );
      });
      
      $('#send').click( function(e) {
      sendImage();
      });

      });

      <!-- ι¬―ο½®υ‚‰„ο½«υο½‰υ€‹Άο½½ο½° ι¬®ο½―υ‚‰„ο½·€ι««ο½²υ‚‰„ο½¤ι²εΈ›θ—¤€θ³η§ο½¬ο½®υ‚‰„ο½£ι™‹ο½Ήυ€ζ™‚υ‚‰„ο½½υ‚‰„ο½½υο½‰υ€‹Άο½½ο½³ι¬®ο½―θ¬«εΈε‘υ€θ–„υ‚‰„ο½½υ‚‰„ο½Ώ€υ€ι©•ο½²θ¶ζ™Άο½½ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άι««ο½΄θ‡ο½Ώ€ηΈΊζ“ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Όι¬―ο½©υ‚‰„ο½¥υ€ι—•εΏ«ο½‰υ€‹Άο½½ο½Έυο½‰υ€‹Άο½½ο½·υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ήυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½°ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§ι¬―ο½®υ‚‰„ο½®ι™‹ο½Ήυ‚‰„ο½Ίυο½‰υ€‹Άο½½ο½©υο½‰υ€‹Άο½½ο½Έυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½½υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½»υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Ίι¬―ο½©θ¬³ο½Ύυ‚‰„ο½½υ‚‰„ο½µυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ίυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½¦ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§‹ -->
      function endPaint() {

      p_flag = false;

      /////////// ι¬―ο½©θ¬³ο½Ύυ‚‰„ο½½υ‚‰„ο½µυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ίυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§ι¬―ο½®υ‚‰„ο½¦υο½‰υ€‹Άο½½ο½®ι«―ο½·ι¬†ι΅ο½ε‚ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½°canvasι¬―ο½©θ¬³ο½Ύυ‚‰„ο½½υ‚‰„ο½µυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ίυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½«ι¬―ο½©θ¬³ο½Ύυ‚‰„ο½½υ‚‰„ο½µυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ίυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½²ι¬―ο½©θ¬³ο½Ύυ‚‰„ο½½υ‚‰„ο½µυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ίυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½¨ι¬―ο½®υ‚‰„ο½―ι΄θ¶£ο½½ο½Άυο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Ύυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½¥ι¬―ο½®υ‚‰„ο½―ι¶ηο½¥υ€”–ο½½ο½½υ‚‰„ο½»ιƒΆο½§θ¬‡ε‡ο½½ο½½υ‚‰„ο½½υο½‰υ€‹Άο½½ο½½υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½« ////////////

      }

      
      <!-- XMLHttpRequest ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άι««ο½΄θ‡ο½Ώ€ιο½±υ‚‰…γ‚‘ο½½ο½§ι¬©ζΎο½½ο½µυο½‰υ€‹Άο½½ο½Ίι«―ζ¨ο½Ίε‡ο½½θ¨—ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Ήυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½§ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½―ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άι««ο½΄θ³ο½»υ€ε‚ι¶ζΊο½»ο½Έυο½‰υ€‹ο½Ώο½«€ι©ο½Έυο½‰υ€‹Άο½½ο½®ι™ο½Άυ‚‰„ο½½ι¨ηΏ«ο½‹υ‚‰„ο½¬υ‚‰„ο½®υο½‰υ€‹Άο½½ο½Ά€υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Ύι¬®ο½―υ‚‰„ο½·ι—”ο½¨θο½²υ‚‰„ο½½υ‚‰„ο½½υο½‰υ€‹Άο½½ο½±ι¬®ο½«υ‚‰„ο½¨ι™‹εΈ¶ζι«£ο½΅υ‚‰„ο½¬υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ήυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§ι¬®ο½£θ›Ήυ€ο½½ο½½υ‚‰„ο½µι««ο½΄θ¬«ο½Ύυ‚‰„ο½½υ‚‰„ο½¶ι¬®ο½―θ®–ο½Έυ‚‰„ο½½υ‚‰„ο½Ίι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§ι¬©ε…ο½½ο½¶ι—•ο½³θ®–ο½Έυ‚‰„ο½½υ‚‰„ο½¥ι›υ€‚®ο½½ο½»υ€ι‚µο½²ι¬¥ο½΄υ‚‰„ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άι««ο½΄ι›ο½£υ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½½υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½¦ι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άι««ο½΄ι›ο½£υ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½½υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Όι¬―ο½©θ·ο½Άυ‚‰„ο½½υ‚‰„ο½Άυυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½§υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½¶ι¬―ο½®υ‚‰„ο½―ι¶ε„ο½½ο½Έυο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½³ι¬®ο½―θ®“ε¥‡ο½½ο½Ίθ››υ€”–ο½½ο½½ιζ¦ο½½εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½½υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Ύυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½©ι¬―ο½―υ‚‰„ο½―υο½‰υ€‹Άο½½ο½®υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½Άυυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½Άι¬―ο½®υ‚‰„ο½«υο½‰υ€‹Άο½½ο½°υυ‚‰„εΏ«€ε…‡υ‚‰„ο½½υ‚‰„ο½¨υυο½‰υ€‹ο½Ώο½«€θυ€”ο½‰υ€‹Άο½½ο½½υο½‰υ€‹Άο½½ο½° -->
      function getXHR() {
      var req;
      try {
      req = new XMLHttpRequest();
      } catch(e) {
      try {
      req = new ActiveXObject("Msxml2.XMLHTTP");
      } catch(e) {
      req = new ActiveXObject("Microsoft.XMLHTTP");
      }
      }
      return req;
      }

