function Send_Answer(){

        req = getXHR();
        // ι¬®ο½±θ®’ο½«ι©ηΆ‘ο½­εΈ¶ο½»ΠΎθΆο½»υ‚‰„ο½Ώυ‚‰„ο½΅ι‚µο½Ίυ‚‰„ο½®ι™·υ€¦ο½½ο½¦ι¨Ύυ€µ
        req.onreadystatechange = function() {


          if( req.readyState == 4 ) {  // 
              if( req.status == 200 ) {   // 
                document.answerAREA.Abox.value = req.responseText;
              } else {                    // ι¬¨ο½ΎθΆο½»υ‚‰„ο½Ώυ‚‰„ο½΅ι™ζ»‚ο½½ο½±ι°ο½¨—
                document.answerAREA.Abox.value = req.status;
              }
          } else {
            document.answerAREA.Abox.value = "wait..."
          }
        }

        // 
        req.open("GET", "/cgi-bin/answer_test.rb?answer=" +
          encodeURIComponent(document.answerAREA.input.value), true);
        req.send(null);
      }


    </script>
    
