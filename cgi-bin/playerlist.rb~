#!/root/.rbenv/shims/ruby
# -*- coding: utf-8 -*-

require 'cgi'

cgi = CGI.new
#header = cgi.header( {"charset"=>"UTF-8"} )
#puts header
#cgi.out { header }

pn_file = "entry_name.dat"

begin
  File.open( pn_file, "r:eucJP'" ) do |f|
    f.each do |line|
      tmp = line.split(",")
      cgi.out{ "#{tmp[1].strip}," }
    end
  end
rescue
  cgi.out{ "error" }
end


